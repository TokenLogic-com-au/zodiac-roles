{
  "name": "permissions-starter-kit",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "setup": "eth-sdk",
    "clean": "rimraf build",
    "apply": "tsc && node build/scripts/apply.js",
    "anvil": "anvil --silent --fork-url https://rpc.ankr.com/eth",
    "test:watch-only": "jest --runInBand --watch",
    "test": "concurrently --raw --kill-others --success first \"yarn anvil\" \"jest --runInBand\"",
    "test:watch": "concurrently --raw --kill-others --handle-input --passthrough-arguments \"yarn anvil\" \"jest --runInBand --watch {@}\" --",
    "check:types": "tsc --noEmit",
    "check:prettier": "prettier '**/*.{js,ts,tsx,json,md}' --list-different",
    "fix:prettier": "yarn prettier '**/*.{js,ts,tsx,json,md}' --write"
  },
  "dependencies": {
    "@dethcrypto/eth-sdk": "^0.3.4",
    "@dethcrypto/eth-sdk-client": "^0.1.6",
    "@gnosis.pm/zodiac": "^3.5.2",
    "@typechain/ethers-v5": "^11.0.0",
    "@types/jest": "^29.5.2",
    "@types/wait-on": "^5.3.1",
    "concurrently": "^8.2.1",
    "ethers": "^5.7.2",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.2.0",
    "typescript": "^5.3.3",
    "wait-on": "^7.0.1",
    "yargs": "^17.7.2",
    "zodiac-roles-sdk": "^2.8.1"
  }
}